// Primer progama.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include "iostream"

using namespace std;

int main ()
{int N,s,i;
cout<<"Introducir N ";
cin>>N;
s=0;
   for(i=1;i<=N;i++)
	   if(i%2!=0)
		   s=s+i;
cout<<"La suma es" << s;
getch();
	return 0;
}

